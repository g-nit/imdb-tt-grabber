{
  "manifest_version": 3,
  "name": "IMDB TT Grabber",
  "version": "1.1",
  "description": "Copy the IMDB tt number from the URL with one click.",
  "permissions": ["clipboardWrite", "activeTab", "scripting"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  }
}
